            "http://www.google.dk/*",
            "http://www.google.es/*",
            "http://www.google.fi/*",
            "http://www.google.lu/*",
            "http://www.google.nl/*",
            "http://www.google.pl/*",
            "http://www.google.uk/*",
            "http://www.google.ch/*",
